      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Build Sequential Skiplist"
    },
    {
