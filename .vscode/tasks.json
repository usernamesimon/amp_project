{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build seq_skiplist",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/src/seq_skiplist.c",
        "-o",
        "${workspaceFolder}/build/seq_skiplist",
        "-I",
        "${workspaceFolder}/inc",
        "-Wall",
        "-Wextra"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Build Sequential Skiplist"
    },
    {
      "type": "cppbuild",
      "label": "Build coarse_skiplist",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/src/coarse_skiplist.c",
        "-o",
        "${workspaceFolder}/build/coarse_skiplist",
        "-I",
        "${workspaceFolder}/inc",
        "-Wall",
        "-Wextra",
        "-fopenmp"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Build Coarse locked Skiplist"
    },
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-Wall",
        "-Wextra",
        "-fopenmp"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}