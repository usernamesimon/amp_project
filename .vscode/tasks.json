    {
      "type": "cppbuild",
      "label": "Build seq_skiplist",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/src/seq_skiplist.c",
        "-o",
        "${workspaceFolder}/build/seq_skiplist",
        "-I",
        "${workspaceFolder}/inc",
        "-Wall",
        "-Wextra"
