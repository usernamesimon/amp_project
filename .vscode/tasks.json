      "label": "build-lock-free-skiplist",
      "type": "shell",
      "command": "gcc",
      "args": [
          "-o",
          "lock_free_skiplist_benchmark123",
          "./src/lock_free_skip_list_benchmark.c",
          "./src/lock_free_skiplist.c",
          "-I/home/kras/Documents/amp_project/inc/",
          "-pthread",
